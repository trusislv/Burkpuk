<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<div class="form-group">
   <div><%= f.label :first_name %><br />
  <%= f.text_field :first_name, autofocus: true, class:"form-control",placeholder:"Vards" %></div>
</div>
<div class="form-group">
   <div><%= f.label :last_name %><br />
  <%= f.text_field :last_name, autofocus: true, class:"form-control",placeholder:"Uzvards" %></div>
</div>
<div class="form-group">
   <div><%= f.label :profile_name %><br />
  <%= f.text_field :profile_name, autofocus: true, class:"form-control",placeholder:"Segvards" %></div>
</div>
<div class="form-group">
  <div><%= f.label :email %><br />
  <%= f.email_field :email, autofocus: true,  class:"form-control",placeholder:"Emails" %></div>

</div>
    




<div class="form-group">
  <div><%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", class:"form-control",placeholder:"parole" %></div>
</div>
<div class="form-group">
  <div><%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", class:"form-control",placeholder:"parole atkartot" %></div>
</div>

  <div  ><%= f.submit "Sign up", class:"btn btn-primary" %></div>
<% end %>


